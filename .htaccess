# Error 400 - Bad Request
ErrorDocument 404 /error404.html

# Error 500 - Internal Server Error
ErrorDocument 500 /error500.html

RewriteEngine On
# Riscrittura URL per Home
RewriteRule ^home/?$ index.html [L]

# Riscrittura URL per Videocorsi
RewriteRule ^videocorsi/?$ videocorsi.html [L]

# Riscrittura URL per Cookie Page
RewriteRule ^cookie-page/?$ cookie-page.html [L]